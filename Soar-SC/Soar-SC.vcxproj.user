<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|Win32'">
    <LocalDebuggerAttach>false</LocalDebuggerAttach>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|Win32'">
    <LocalDebuggerSQLDebugging>false</LocalDebuggerSQLDebugging>
    <DebuggerFlavor>WindowsLocalDebugger</DebuggerFlavor>
    <LocalDebuggerCommand>$(SolutionDir)RunChaosLauncher.exe</LocalDebuggerCommand>
    <LocalDebuggerWorkingDirectory>$(ProgramFiles)\Starcraft</LocalDebuggerWorkingDirectory>
    <ShowAllFiles>false</ShowAllFiles>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|Win32'">
    <LocalDebuggerCommand>$(ProjectDir)RunChaosLauncher.exe</LocalDebuggerCommand>
    <DebuggerFlavor>WindowsLocalDebugger</DebuggerFlavor>
    <LocalDebuggerWorkingDirectory>$(ProgramFiles)\Starcraft</LocalDebuggerWorkingDirectory>
    <LocalDebuggerAttach>false</LocalDebuggerAttach>
    <LocalDebuggerSQLDebugging>false</LocalDebuggerSQLDebugging>
  </PropertyGroup>
</Project>