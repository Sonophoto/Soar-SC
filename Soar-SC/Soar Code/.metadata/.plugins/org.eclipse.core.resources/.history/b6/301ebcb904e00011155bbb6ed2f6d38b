sp {global*increment-cycle-count
   (state <s> ^operator <op>
              ^cycle-count <c>)
-->
   (<c> 

sp {remove*output-commands
   (state <s> ^name Soar-SC
              ^operator <op>
              ^io.output-link <o>
              ^cycle-count <current>)
   (<o> ^<attr> <object>)
   (<object> ^cycle <> <current>)
-->
   (<o> ^<attr> <object> -)
}
