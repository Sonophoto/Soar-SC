sp {apply*build-supply-depot
   (state <s> ^operator <op>
              ^io.input-link <i>
              ^io.output-link <o>)
   (<op> ^name build-supply-depot
         ^worker <id>
         ^location-x <x>
         ^locaation-y <y>)
   (<i> ^types.type <t>
        ^supply-depots <number>)
   (<t> ^name |Terran Supply Depot|
        ^id <type-id>)
-->
   (<o> ^build <b>)
   (<b> ^type <type-id>
        ^location-x <x>
        ^location-y <y>)
   (<s> ^supply-depots <number> -
        ^supply-depots (+ <number))
}