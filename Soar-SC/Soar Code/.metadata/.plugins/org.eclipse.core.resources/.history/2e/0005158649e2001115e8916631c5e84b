sp {apply*build-supply-depot
   (state <s> ^operator <op>
              ^io.input-link <i>
              ^io.output-link <o>)
   (<op> ^name build-supply-depot
         ^worker <id>
         ^location-x <x>
         ^location-y <y>)
   (<i> ^types.type <t>
        ^supply-depots <number>)
   (<t> ^name |Terran Supply Depot|
        ^id <type-id>)
-->
   (<o> ^build <b>)
   (<b> ^type <type-id>
        ^location-x <x>
        ^location-y <y>
        ^worker <id>)
   (<i> ^supply-depots <number> -
        ^supply-depots (+ <number> 1))
}