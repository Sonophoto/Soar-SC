sp {propose*build-barracks
   (state <s> ^name Soar-SC
              ^superstate nil
              ^io.input-link <i>)
   (<i> ^minerals <m> > 99
        ^units.unit <u>
        ^types.type <t>
        ^supply-depots { <l> < 9 })
   (<t> ^name |Terran Barracks|
        ^id <type-id>)
   (<u> ^type <type-id>
        ^idle <idle>
        ^carrying <carrying>
        ^id <id>)
-->
   (<s> ^operator <o> + =)
   (<o> ^name build-barracks
        ^worker <id>
        ^idle <idle>
        ^carrying <carrying>
        ^location <l>)
}

sp {elaboration*remove-build-supply-barracks-operator-if-already-building
   (state <s> ^operator <op> +
              ^io.input-link.units.unit <u>)
   (<op> ^name build-barracks)
   (<u> ^constructing 1)
-->
   (<s> ^operator <op> -)
}

sp {elaboration*building-supply-barracks-better-than-build-scv
   (state <s> ^operator <o1> +
              ^operator <o2> +)
   (<o1> ^name build-barracks)
   (<o2> ^name build-scv)
-->
   (<s> ^operator <o1> > <o2>)
}

sp {elaboration*building-supply-depot-better-than-gather-minerals
   (state <s> ^operator <o1> +
              ^operator <o2> +)
   (<o1> ^name build-supply-depot)
   (<o2> ^name gather-minerals)
-->
   (<s> ^operator <o1> > <o2>)
}

sp {elaboration*building-supply-depot-idle-better-than-non-idle
   (state <s> ^operator <o1> +
              ^operator <o2> +)
   (<o1> ^name build-supply-depot
         ^idle 1)
   (<o2> ^name build-supply-depot
         ^idle 0)
-->
   (<s> ^operator <o1> > <o2>)
}

sp {elaboration*building-supply-depot-non-carrying-better-than-carrying
   (state <s> ^operator <o1> +
              ^operator <o2> +)
   (<o1> ^name build-supply-depot
         ^carrying 0)
   (<o2> ^name build-supply-depot
         ^carrying 1)
-->
   (<s> ^operator <o1> > <o2>)
}

sp {elaboration*location-to-x-y-0
   (state <s> ^operator <o>)
   (<o> ^name build-supply-depot
        ^location 0)
-->
   (<o> ^location-x 42
        ^location-y 47)
}

sp {elaboration*location-to-x-y-1
   (state <s> ^operator <o>)
   (<o> ^name build-supply-depot
        ^location 1)
