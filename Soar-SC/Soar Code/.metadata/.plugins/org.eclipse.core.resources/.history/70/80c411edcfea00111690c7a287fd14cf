sp {propose*build-supply-depot
   (state <s> ^name Soar-SC
              ^superstate nil
              ^io.input-link <i>)
   (<i> ^minerals <m> > 99
        ^units.unit <u>
        ^types.type <t>
        ^types.type <t-s>
        ^supply-depots { <l> < 9 })
   (<t> ^name |Terran SCV|
        ^id <type-id>)
   (<u> ^type <type-id>
        ^idle <idle>
        ^carrying <carrying>
        ^id <id>)
   (<t-s> ^name |Terran Supply Depot|
          ^id <supply-id>)
-->
   (<s> ^operator <o> + =)
   (<o> ^name build-building
        ^worker <id>
        ^idle <idle>
        ^carrying <carrying>
        ^location <l>
        ^type <supply-id>)
}

sp {elaboration*remove-build-supply-depot-operator-if-already-building
   (state <s> ^operator <op> +
              ^io.input-link <i>)
   (<i> ^units.unit <u>
        ^types.type <t>)
   (<t> ^name |Terran Supply Depot|
        ^id <type-id>)
   (<op> ^name build-building
         ^type <type-id>)
   (<u> ^constructing <type-id>)
-->
   (<s> ^operator <op> -)
}

sp {elaboration*remove-build-supply-depot-operator-if-not-yet-building-but-will
   (state <s> ^operator <op> +
              ^io.output-link.build <b>
              ^io.input-link.types.type <t>)
   (<t> ^name |Terran Supply Depot|
        ^id <type-id>)
   (<b> ^type <type-id>
       -^status)
   (<op> ^name build-supply-depot)
-->
   (<s> ^operator <op> -)
}

sp {elaboration*building-supply-depot-better-than-gather-minerals
   (state <s> ^operator <o1> +
              ^operator <o2> +)
   (<o1> ^name build-supply-depot)
   (<o2> ^name gather-minerals)
-->
   (<s> ^operator <o1> > <o2>)
}

sp {elaboration*building-supply-depot-better-than-build-barracks
   (state <s> ^operator <o1> +
              ^operator <o2> +)
   (<o1> ^name build-supply-depot)
   (<o2> ^name build-barracks)
-->
   (<s> ^operator <o1> > <o2>)
}

sp {elaboration*supply-depots*location-to-x-y-0
   (state <s> ^operator <o>)
   (<o> ^name build-supply-depot
        ^location 0)
-->
   (<o> ^location-x 42
        ^location-y 47)
}

