sp {
   (state <s> ^operator <op>
              ^io.input-link <i>
              ^io.output-link <o>
              ^buildings <b>)
   (<op> ^name build-building
         ^worker <id>
         ^location-x <x>
         ^location-y <y>
         ^type <type-id>)
   (<i> ^types.type <t>)
   (<b> ^building <building>)
   (<building> ^type <type-id>
               ^count <c>)
-->
   (<o> ^build <b>)
   (<b> ^type <type-id>
        ^location-x <x>
        ^location-y <y>
        ^worker <id>)
   (<i> ^supply-depots <number> -
        ^supply-depots (+ <number> 1))
}