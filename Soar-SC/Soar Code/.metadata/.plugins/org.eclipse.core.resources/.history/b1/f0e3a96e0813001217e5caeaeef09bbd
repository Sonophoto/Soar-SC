sp {propose*initialize-soar-sc
   (state <s> ^superstate nil
             -^name
             -^cycle-count)
-->
   (<s> ^operator <o> + >)
   (<o> ^name initialize-soar-sc)
}

sp {apply*initialize-soar-sc
   (state <s> ^operator.name initialize-soar-sc
              ^io.input-link <i>)
   (<i> ^types.type <t-b>
        ^types.type <t-s>
        ^types.type <t-scv>)
   (<t-b> ^name |Terran Barracks| 
          ^id <barracks-id>)
   (<t-s> ^name |Terran Supply Depot|
          ^id <supply-id>)
   (<t-scv> ^name |Terran SCV|
            ^id <scv-type>)
-->
   (<s> ^name Soar-SC
   	    ^buildings <b>
        ^unit-limits <u-l>
        ^minerals-used 0
        ^gas-used 0)
   (<b> ^building <barracks>
        ^building <supply>)
   (<u-l> ^unit-limit <scv-limit>
          ^unit-limit <marine-limit>)
   (<barracks> ^type <barracks-id>
               ^count 0)
   (<supply> ^type <supply-id>
             ^count 0)
   (<scv-limit> ^type <scv-type>
                ^count 0)
}