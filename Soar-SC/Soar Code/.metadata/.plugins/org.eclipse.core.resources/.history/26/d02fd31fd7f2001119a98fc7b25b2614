   (state <s> ^name Soar-SC
              ^operator <op>
              ^io.output-link <o>
              ^io.input-link.types.type <object-type>)
   (<o> ^<attr> <object>)
   (<object> ^status
             ^type <type-id>)
   (<object-type> ^id <type-id>
                  ^mineral-cost <mineral-cost>
                  ^gas-cost <gas-cost>)
-->
   (<o> ^<attr> <object> -)
}
