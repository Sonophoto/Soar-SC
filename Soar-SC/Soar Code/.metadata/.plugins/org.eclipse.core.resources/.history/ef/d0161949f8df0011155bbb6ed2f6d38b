sp {propose*build-supply-depot
   (state <s> ^name Soar-SC
              ^superstate nil
              ^io.input-link <i>
              ^location <l>)
   (<i> ^minerals <m> > 99
        ^units.unit <u>
        ^types.type <t>)
   (<t> ^name |Terran SCV|
        ^id <type-id>)
   (<u> ^type <type-id>
        ^idle <idle>
        ^carrying <carrying>
        ^id <id>)
-->
   (<s> ^operator <o> + =)
   (<o> ^name build-supply-depot
        ^worker <id>
        ^idle <idle>
        ^carrying <carrying>
        ^location <l>)
}

sp {elaboration*building-supply-depot-better-than-build-scv
   (state <s> ^operator <o1>
              ^operator <o2>)
   (<o1> ^name build-supply-depot)
   (<o2> ^name build-scv)
-->
   (<s> ^operator <o1> > <o2>)
}

sp {elaboration*building-supply-depot-better-than-gather-minerals
   (state <s> ^operator <o1>
              ^operator <o2>)
   (<o1> ^name build-supply-depot)
   (<o2> ^name gather-minerals)
-->
   (<s> ^operator <o1> > <o2>)
}
 
sp {elaboration*building-supply-depot-idle-better-than-non-idle
   (state <s> ^operator <o1>
              ^operator <o2>)
   (<o1> ^name build-supply-depot
         ^idle 1)
   (<o2> ^name build-supply-depot
         ^idle 0)
-->
   (<s> ^operator <o1> > <o2>)
}

sp {elaboration*building-supply-depot-non-carrying-better-than-carrying
   (state <s> ^operator <o1>
              ^operator <o2>)
   (<o1> ^name build-supply-depot
         ^carrying 0)
   (<o2> ^name build-supply-depot
         ^carrying 1)
-->
   (<s> ^operator <o1> > <o2>)
}

sp {elaboration*location-to-x-y-0
   (state <s> ^operator <o>)
   (<o> ^name build-supply-depot
        ^location 0
       -^location-x
       -^location-y)
-->
   (<o> ^location-x 42
        ^location-y 47)
}

sp {elaboration*location-to-x-y-1
   (state <s> ^operator <o>)
   (<o> ^name build-supply-depot
        ^location 1
       -^location-x
       -^location-y)
-->
   (<o> ^location-x 45
        ^location-y 47)
}

sp {elaboration*location-to-x-y-0
   (state <s> ^operator <o>)
   (<o> ^name build-supply-depot
        ^location 0
       -^location-x
       -^location-y)
-->
   (<o> ^location-x 42
        ^location-y 47)
}

